@charset "utf-8";
/* CORRECCIONES */
::selection { background:#555; color:#fff; }/* Cambio del color de seleccion:webkit, opera, IE9 */
::-moz-selection { background:#555; color #fff; }/* Cambio del color de seleccion:firefox */
[required]{background-color: #FFF !important;}/* marca de color distinto los input obligatorios */
:invalid{background-color: #F7F7F6;}/* destaca los input con datos mal ingresados */
input:-webkit-autofill {-webkit-box-shadow: 0 0 0px 1000px white inset;}/* quita el color amarillo de los input en webkit */
[disabled]{background-color: #CCC !important;}/* marca de color distinto los input desactivados */
/* FIN CORRECCIONES */
